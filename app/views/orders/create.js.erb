<% if @failed -%>
$("#flash_notice").hide(300);
$("#form_have_errors").html("<%= escape_javascript(flash.delete(:error))%>");
$("#form_have_errors").show(300);
$('#processing_after').hide();
$('#processing_before').show();
<% else -%>
<%flash.discard%>
$("#order_form_status").hide(300);
$("#ajax_response_container").html("<%= escape_javascript( render(:template => "physician_groups/edit") ) %>");

validateNameAndEmail();
handleRemoveElement();
check_if_physician_itself($('#physician_group_physician_itself'));
$('#physician_group_physician_itself').click(function() {
    check_if_physician_itself(this);
})
<% end -%>



